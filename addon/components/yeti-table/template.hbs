{{yield (hash
  header=(component "yeti-table/header"
    columns=columns onColumnClick=(action "onColumnSort")
    sortings=sortings parent=this
  )
  body=(component "yeti-table/body" data=processedData columns=columns parent=this)
  actions=(hash
    previousPage=(action "previousPage")
    nextPage=(action "nextPage")
    goToPage=(action "goToPage")
    changePageSize=(action "changePageSize")
  )
  paginationData=paginationData
  isLoading=isLoading
)}}