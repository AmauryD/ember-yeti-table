{
  "name": "ember-yeti-table",
  "version": "1.7.3",
  "description": "Lightweight and flexible tables addon.",
  "keywords": [
    "ember-addon",
    "table",
    "tables",
    "datagrid",
    "yeti"
  ],
  "repository": "https://github.com/miguelcobain/ember-yeti-table",
  "license": "MIT",
  "author": "Miguel Andrade <jmandrade0@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "deploy": "ember deploy production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "eslint-config-check": "eslint --print-config . | eslint-config-prettier-check",
    "format:prettier": "prettier --write \"**/*.{js,scss,json,yaml,yml}\"",
    "format:eslint": "eslint --fix \"**/*.js\"",
    "format:all": "run-s format:prettier format:eslint"
  },
  "dependencies": {
    "deepmerge": "^4.3.0",
    "ember-async-data": "^1.0.2",
    "ember-auto-import": "^2.6.1",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^5.7.2",
    "ember-modifier": "^4.0.0",
    "ember-resources": "^6.4.0",
    "tracked-toolbox": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.9.3",
    "@embroider/test-setup": "^0.48.1",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@glint/template": "1.0.0-beta.3",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "broccoli-funnel": "^3.0.8",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-source": "^3.0.1",
    "ember-array-helper": "^5.1.0",
    "ember-cli": "~3.28.6",
    "ember-cli-addon-docs": "^4.2.2",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-dependency-lint": "^2.0.1",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-favicon": "^3.0.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-mirage": "3.0.0-alpha.3",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-concurrency": "^2.3.7",
    "ember-concurrency-decorators": "^2.0.3",
    "ember-data": "^3.28.13",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "^8.1.2",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-page-title": "^6.2.2",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.1.0",
    "ember-source": "~3.28.11",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.16.0",
    "ember-truth-helpers": "^2.1.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-ember": "^10.6.1",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-qunit": "^6.2.0",
    "faker": "^5.5.3",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "qunit": "^2.19.4",
    "qunit-dom": "^1.6.0",
    "sinon": "^9.2.4",
    "webpack": "^5.75.0"
  },
  "resolutions": {
    "ember-cli-clipboard": "0.13.0"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://miguelcobain.github.io/ember-yeti-table"
  },
  "homepage": "https://miguelcobain.github.io/ember-yeti-table",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,scss,json,yaml,yml,md}": "prettier --write",
    "*.js": "eslint --fix"
  }
}
